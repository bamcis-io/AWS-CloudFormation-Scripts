            "exclusions": {
              "Fn::Sub": "[\"${SourceS3Location}**/CloudTrail-Digest/**\"]"
            },
            "jsonPath": "$.Records[*]",
            "compressionType": "gzip",
            "typeOfData": "file"
          },
