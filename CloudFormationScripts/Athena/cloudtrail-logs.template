    "GlueETLPartitionedTable": {
      "Type": "AWS::Glue::Table",
      "Properties": {
        "CatalogId": {
          "Ref": "AWS::AccountId"
        },
        "DatabaseName": {
          "Ref": "Database"
        },
        "TableInput": {
          "Description": "Table containing CloudTrail Logs to be used with Glue ETL operations",
          "Name": "cloudtrail_logs_glue_etl_source_partitioned",
          "TableType": "EXTERNAL_TABLE",
          "Parameters": {
